@model IEnumerable<PhoneBill.Models.Supervisor>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SupervisorName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SupervisorEmail)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SupervisorPhone)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UnitesSectionsId)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SupervisorName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SupervisorEmail)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SupervisorPhone)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UnitesSectionsId)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
